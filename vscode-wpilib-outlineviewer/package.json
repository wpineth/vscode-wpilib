{
    "name": "vscode-wpilib-outlineviewer",
    "displayName": "vscode-wpilib-outlineviewer",
    "description": "Outline Viewer runner for wpilib",
    "version": "0.0.1",
    "publisher": "wpifirst",
    "engines": {
        "vscode": "^1.20.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.sayHello",
                "title": "Hello World"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.7.2",
        "vscode": "^1.1.13",
        "tslint": "^5.9.1",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    }
}
