{
  "name": "build-projects",
  "version": "1.0.0",
  "description": "A minimal Electron application written with Typescript",
  "scripts": {
    "compile": "tsc",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "installProjects": "node ./out/main.js -i",
    "compileProjects": "node ./out/main.js -c",
    "packageProjects": "node ./out/main.js -p",
    "start": "npm run compile && node ./out/main.js"
  },
  "main": "out/main.js",
  "repository": "https://github.com/electron/electron-quick-start-typescript",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo",
    "typescript"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "tslint": "^5.9.1",
    "typescript": "^2.7.2"
  },
  "dependencies": {
    "@types/glob": "^5.0.35",
    "@types/mkdirp": "^0.5.2",
    "@types/ncp": "^2.0.1",
    "@types/npm": "^2.0.29",
    "@types/yargs": "^11.0.0",
    "glob": "^7.1.2",
    "jsonc-parser": "^1.0.3",
    "mkdirp": "^0.5.1",
    "ncp": "^2.0.0",
    "npm": "^5.8.0",
    "vsce": "^1.37.6",
    "yargs": "^11.0.0"
  }
}
