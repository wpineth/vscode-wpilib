        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.7.2",
        "vscode": "^1.1.13",
        "tslint": "^5.9.1",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    },
    "dependencies": {
        "@types/glob": "^5.0.35",
        "@types/mkdirp": "^0.5.2",
        "@types/ncp": "^2.0.1",
        "glob": "^7.1.2",
        "jsonc-parser": "^1.0.3",
        "mkdirp": "^0.5.1",
        "ncp": "^2.0.0"
    }
}
