{
    "name": "vscode-wpilib-java",
    "displayName": "vscode-wpilib-java",
    "description": "Vscode for WPILib Java",
    "version": "0.0.11",
    "publisher": "wpifirst",
    "engines": {
        "vscode": "^1.20.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./out/extension",
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
